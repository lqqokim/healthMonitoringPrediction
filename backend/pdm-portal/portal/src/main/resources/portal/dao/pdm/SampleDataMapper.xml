<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bistel.a3.portal.dao.pdm.SampleDataMapper">




    <select id="selectSampleDataByRawId" resultType="com.bistel.a3.portal.domain.pdm.db.SampleData">
        select *
        from sample_data
        where rawid=#{rawId}
    </select>




</mapper>