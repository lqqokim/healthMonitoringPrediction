{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "removeComments": true,
        "noLib": false,
        "lib": ["es5", "es2015", "es2016", "dom"],
        "declaration": true,
        "noImplicitAny": false,
        "noImplicitReturns": false,
        "noImplicitUseStrict": false,
        "noFallthroughCasesInSwitch": false,
        "allowSyntheticDefaultImports": false,
    }
}
